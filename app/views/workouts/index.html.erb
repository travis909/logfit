<h1>Workouts</h1>

<div class='row'>
  <div class="col-md-6">
    <h1>Register</h1>
    <%= form_for :workout do |f| %>
      <div class="form-group">
        <%= f.label :date %>
        <%= f.date_field :date %>
      </div>
      <%= f.submit %>
    <% end %>
  </div>
</div>

<%= link_to "Log Out", "/log_out" %>